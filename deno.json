{
  "nodeModulesDir": "manual",
  "tasks": {
    "check": "deno fmt --check . && deno lint . && deno check",
    "dev": "vite",
    "build": "vite build",
    "start": "deno serve -A _fresh/server.js",
    "update": "deno run -A -r jsr:@fresh/update ."
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "imports": {
    "@/": "./",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^1.0.8",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.18",
    "cheerio": "npm:cheerio@^1.2.0",
    "daisyui": "npm:daisyui@^5.5.18",
    "lucide-preact": "npm:lucide-preact@^0.562.0",
    "preact": "npm:preact@^10.28.3",
    "@preact/signals": "npm:@preact/signals@^2.7.1",
    "tailwindcss": "npm:tailwindcss@^4.1.18",
    "fresh": "jsr:@fresh/core@^2.2.0",
    "tw-animate-css": "npm:tw-animate-css@^1.4.0",
    "vite": "npm:vite@^7.3.1",
    "vite-plugin-lucide-preprocess": "npm:vite-plugin-lucide-preprocess@^1.4.6"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ],
    "types": [
      "vite/client"
    ]
  },
  "deploy": {
    "project": "c46d0d83-72ff-411f-b5c6-6f2bd6e9ae3d",
    "exclude": [
      "**/node_modules"
    ],
    "include": [],
    "entrypoint": "main.ts"
  },
  "allowScripts": [
    "npm:@swc/core@1.7.26",
    "npm:esbuild@0.25.7",
    "npm:esbuild@0.27.2"
  ]
}